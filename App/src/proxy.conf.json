{
	"/api/*": {
		"target": "http://localhost:8082",
		"secure": false,
		"logLevel": "debug",
		"changeOrigin": false,
		"pathRewrite": {
			"^/api/*": "/"
		  }
	},

	"/stream/*": {
		"target": "wss://localhost:7284/ws",
		"secure": true,
		"ws": true,
		"changeOrigin": false,
		"pathRewrite": {
			"^/api/*": "/"
		  }
	  }
}
